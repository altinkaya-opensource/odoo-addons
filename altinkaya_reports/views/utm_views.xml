<?xml version="1.0" encoding="UTF-8"?>
<odoo>
<data>
		<record model="ir.ui.view" id="view_utm_campaign_form_inherit_altinkaya">
        <field name="name">utm.campaign.form.inherit.altinkaya</field>
        <field name="model">utm.campaign</field>
        <field name="inherit_id" ref="utm.utm_campaign_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form" position="inside">
                <notebook>
                    <page string="Partners" name="partners">
                        <field name="partner_ids" widget="many2many_list"/>
                    </page>
                </notebook>
            </xpath>
        </field>
    </record>



</data>
</odoo>