<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="account_view_move_form_fedex" model="ir.ui.view">
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="carrier_id" invisible="1"/>
                <field name="delivery_type" invisible="1"/>
            </field>
            <xpath expr="//button[@name='button_set_checked']" position="after">
                <button name="get_fedex_rates" string="Get FedEx Rates" type="object" attrs="{'invisible': [('delivery_type', '!=', 'fedex')]}"/>
            </xpath>
            <xpath expr="//button[@name='button_set_checked']" position="after">
                <button name="send_fedex_shipping" string="Send w/ FedEx Shipping" type="object" attrs="{'invisible': [('delivery_type', '!=', 'fedex')]}"/>
            </xpath>
        </field>
    </record>
</odoo>

